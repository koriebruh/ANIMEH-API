###
GET http://localhost:9200/anime_info/_search
Accept: application/json
Content-Type: application/json

{
  "from": 0,
  "size": 20,
  "query": {
    "bool": {
      "must": [
        {
          "match": {
            "name": {
              "query": "",
              "fuzziness": "AUTO"
            }
          }
        }
      ],
      "should": [
        {
          "term": {
            "genres": "Comedy"
          }
        },
        {
          "bool": {
            "must_not": {
              "exists": {
                "field": "genres"
              }
            }
          }
        },
        {
          "range": {
            "score": {
              "gte": 8  // Nilai score lebih besar atau sama dengan 8.0
            }
          }
        },
        {
          "bool": {
            "must_not": {
              "exists": {
                "field": "score"
              }
            }
          }
        },
        {
          "term": {
            "status": "Finished Airing"  // Memfilter berdasarkan status
          }
        }
      ]
    }
  }
}

